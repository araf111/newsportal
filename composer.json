{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1.0",
        "devianl2/laravel-scorm": "^4.0",
        "doctrine/dbal": "^3.5",
        "firebase/php-jwt": "^6.4",
        "guzzlehttp/guzzle": "^7.4",
        "guzzlehttp/psr7": "^2.5",
        "intervention/image": "^2.4",
        "ivanomatteo/laravel-device-tracking": "^0.2.1",
        "iyzico/iyzipay-php": "^2.0",
        "joisarjignesh/bigbluebutton": "^2.3",
        "jubaer/zoom-laravel": "^1.2",
        "ladumor/laravel-pwa": "^0.0.3",
        "laravel/framework": "^10.0",
        "laravel/passport": "^11.8",
        "laravel/sanctum": "^3.0.0",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.5.2",
        "league/flysystem-aws-s3-v3": "3.0",
        "mercadopago/dx-php": "2.5.3",
        "mews/purifier": "^3.3",
        "mollie/laravel-mollie": "^2.19",
        "php-http/message": "^1.16",
        "php-http/message-factory": "^1.1",
        "psr/simple-cache": "^3.0",
        "pusher/pusher-php-server": "^7.2",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-cookie-consent": "^3.2",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-sitemap": "^6.3",
        "unicodeveloper/laravel-paystack": "^1.0",
        "vimeo/vimeo-api": "^3.0",
        "yoeunes/toastr": "^2.3.5",
        "yajra/laravel-datatables-oracle": "^10.0",
        "illuminate/contracts": "^10.0",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^2.8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files" : [
            "app/Helper/coreconstant.php",
            "app/Helper/corearray.php",
            "app/Helper/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
